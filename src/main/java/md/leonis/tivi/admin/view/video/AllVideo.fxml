<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="md.leonis.tivi.admin.view.video.AddVideoController">
    <children>
        <HBox alignment="BOTTOM_LEFT">
            <children>
                <Label text="Вставьте URL-адрес ссылки1111111111111111111:" />
            </children>
        </HBox>
        <TextField fx:id="urlTextField" promptText="https://www.youtube.com/watch?v=TUAy0WKsqOs" />
        <HBox alignment="BOTTOM_RIGHT">
            <children>
                <Button fx:id="nextButton" mnemonicParsing="false" onAction="#processVideo" text="Далее" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
    </padding>
</VBox>
